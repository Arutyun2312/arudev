{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "npm: dev",
            "type": "shell",
            "command": "npm run dev",
            "isBackground": true,
            "problemMatcher": {
                "owner": "custom",
                "pattern": [
                    {
                        "regexp": "Ready in",
                        "file": 1,
                        "location": 2,
                        "message": 0
                    }
                ],
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": ".",
                    "endsPattern": "Ready in"
                }
            }
        }
    ]
}